includes:
  # WordPress extension is auto loaded by extension installer

parameters:
  level: 0  # start gentle, you can bump this later

  paths:
    - cashu-for-woocommerce.php
    - src

  excludePaths:
    - vendor/*
    - node_modules/*
    - tests/*
    - assets/*
    - languages/*

  bootstrapFiles:
    # Optional, but useful for WooCommerce symbols
    - vendor/php-stubs/woocommerce-stubs/woocommerce-stubs.php
